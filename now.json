{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "methods": ["GET"],
      "headers": { "cache-control": "s-maxage=30, stale-while-revalidate" }
    },
    {
      "src": "/posts/.*",
      "methods": ["GET"],
      "headers": { "cache-control": "s-maxage=30, stale-while-revalidate" }
