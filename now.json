{
  "version": 2,
  "routes": [
    {
      "src": "/",
      "methods": ["GET"],
      "headers": {
        "cache-control": "s-maxage=60, max-age=0, stale-while-revalidate"
      }
    },
    {
      "src": "/posts/.*",
      "methods": ["GET"],
      "headers": {
        "cache-control": "s-maxage=60, max-age=0, stale-while-revalidate"
      }
    },
    {
      "src": "/api/.*",
      "methods": ["GET"],
      "headers": {
        "cache-control": "s-maxage=60, max-age=0, stale-while-revalidate"
      }
    }
  ]
}
